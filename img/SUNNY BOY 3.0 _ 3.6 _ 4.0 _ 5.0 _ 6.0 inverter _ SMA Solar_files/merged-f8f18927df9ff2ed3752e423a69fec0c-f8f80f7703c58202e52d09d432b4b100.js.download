
/* globals $ */

(function () {

    "use strict";

    /**
     * Manages the opened/closed states of the displayed hotspots
     * depending on the selected mode
     *
     * @constructor
     */
    function Hotspot(container) {
        /**
         * local object availability
         * @private
         */
        var _this = this;

        this.mode = 'default';

        this.$allHotspots = null;

        this.$container = $(container);

        /**
         * @method init
         * @returns {undefined}
         */
        this.init = function () {

            // Set global variables
            _this.mode = _this.$container.data('hotspotMode');
            _this.$allHotspots = _this.$container.find('[data-single-hotspot-wrapper]');

            // Bind events for each hotspot
            _this.bindHotspotEvents();

            _this.bindVideoEndedEvent();

            _this.bindHotspotModeChangedEvent();
        };

        this.bindVideoEndedEvent = function() {
            if (_this.$container.data('displayHotspotsOnVideoEnd') !== undefined) {
                _this.$container.on('videoEnded', function () {
                    _this.$container.removeClass('hide-all-hotspots');
                });
            }
        };

        /**
         * Triggers event binding depending on selected mode
         */
        this.bindHotspotEvents = function () {
            _this.$allHotspots.each(function (index, hotspot) {
                var $hotspot = $(hotspot);
                var $wrapper = $($hotspot.find('.hotspot-wrapper'));
                var $button = $($hotspot.find('.btn-extend'));

                switch (_this.mode) {
                    case 'multiple-open':
                        _this.bindMultipleOpenClickEvent($hotspot, $wrapper, $button);
                        break;

                    case 'initial-open-all':
                        _this.toggleAllHotspots(true);
                        _this.bindMultipleOpenClickEvent($hotspot, $wrapper, $button);
                        break;

                    default:
                        // 'single-open' hotspot mode
                        _this.bindSingleOpenClickEvent($hotspot, $wrapper, $button);
                }
            });
        };

        /**
         * Selects a new mode and changes the event bindings accordingly
         */
        this.bindHotspotModeChangedEvent = function () {
            $(document).on("hotspotModeChanged", function (event, mode) {

                _this.unbindAllEvents();
                _this.mode = mode;

                // Close all hotspots
                _this.toggleAllHotspots(false);

                _this.bindHotspotEvents();
            });
        };

        /**
         * Resets all event bindings
         */
        this.unbindAllEvents = function () {
            _this.$allHotspots.each(function (index, hotspot) {
                $(hotspot).unbind();
                $(hotspot).find('.hotspot-wrapper').unbind();
                $(hotspot).find('.btn-extend').unbind();
            });
        };

        /**
         * 'single-open' mode
         * closes all hotspots before opening a new one
         *
         * @param $hotspot
         * @param $wrapper
         * @param $button
         */
        this.bindSingleOpenClickEvent = function ($hotspot, $wrapper, $button) {
            $button.on('click', function (e) {
                e.preventDefault();
                e.stopPropagation();
                var isHotspotClosed = $wrapper.hasClass('collapsed');
                _this.toggleAllHotspots(false);
                // only toggle the hotspot if it hasn't been toggled during reset of all hotspots
                if (isHotspotClosed) {
                    _this.toggleSingleHotspot($hotspot, $wrapper, $button);
                }
            });
        };

        /**
         * 'multi-open' mode (also used for 'initial-open-all' mode)
         * toggles the state of the selected hotspot
         *
         * @param $hotspot
         * @param $wrapper
         * @param $button
         */
        this.bindMultipleOpenClickEvent = function ($hotspot, $wrapper, $button) {
            $button.on('click', function (e) {
                e.preventDefault();
                e.stopPropagation();
                _this.toggleSingleHotspot($hotspot, $wrapper, $button);
            });
        };

        /**
         * Toggles a single hotspot by setting classes on the given elements
         *
         * @param $hotspot
         * @param $wrapper
         * @param $button
         */
        this.toggleSingleHotspot = function ($hotspot, $wrapper, $button) {
            $hotspot.toggleClass('active-hotspot');
            $wrapper.toggleClass('collapsed');
            $button.toggleClass('collapsed');
        };

        /**
         * Toggles all hotspots by setting classes on the given elements
         *
         * @param openHotspots
         */
        this.toggleAllHotspots = function (openHotspots) {
            _this.$allHotspots.toggleClass('active-hotspot', openHotspots);
            _this.$container.find('.hotspot-wrapper').toggleClass('collapsed', !openHotspots);
            _this.$container.find('.btn-extend').toggleClass('collapsed', !openHotspots);
        };

        _this.init();
    }

    window.SMA = window.SMA || {};

    window.SMA.Hotspot = Hotspot;
})();
$(document).ready(function () {
    "use strict";

    $('[data-hotspot-container]').each(function (index, container) {
        var hotspot = new window.SMA.Hotspot(container);
    });
});
