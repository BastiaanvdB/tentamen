window.LIGHTWERK = window.LIGHTWERK || {};

(function ($, LW) {
    'use strict';

    LW.LwSmaTracker = {
        init: function () {
            $('.lw-sma-tracking').bind('click', function () {
                LW.LwSmaTracker.gaTrack(
                    $(this).data('tracking')
                );
            });
            $('.lw-sma-tracking-pageview').bind('click', function (e) {
                ga('send', 'pageview', $(this).data('location'));
            });
        },

        gaTrack: function (trackingData) {
            //LW.LwSmaTracker.log(trackingData);
            ga('send', 'event', trackingData.category, trackingData.action, trackingData.label, 1);
        },

        log: function (trackingData) {
            console.log('category: ' + trackingData.category);
            console.log('action: ' + trackingData.action);
            console.log('label: ' + trackingData.label);
        }
    };

    $(function () {
        LW.LwSmaTracker.init();
    });
