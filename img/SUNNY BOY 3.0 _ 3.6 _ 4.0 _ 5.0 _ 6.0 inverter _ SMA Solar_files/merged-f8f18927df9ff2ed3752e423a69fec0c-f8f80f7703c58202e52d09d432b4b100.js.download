window.LIGHTWERK = window.LIGHTWERK || {};

(function ($, LW) {
    'use strict';

    LW.LwSmaContacts = {
        changeCountry: function($container, countryCode) {
            var $tab = $container.find('.company-location[data-country-code="' + countryCode.toUpperCase() + '"]')
                                 .closest('.tab-pane');
            if ($tab !== undefined && $tab.length && !$tab.hasClass('active')) {
                $container.find('.tab-pane').removeClass('active in');
                $tab.addClass('active in');

                // v2
                $container.find('.tab-selecter-options > li').removeClass('active');
                var $selectorOption = $container.find('.tab-selecter-item[href="#' + $tab.attr('id') +'"]');
                $selectorOption.parent().addClass('active');
                $container.find('.tab-selecter-selected').html($selectorOption.html());

                // v3
                var selectBox = $('#contact-select');
                if (selectBox.length) {
                    $('option', selectBox).removeAttr('selected');
                    var option = $('option[value="#' + $tab.attr('id') + '"]', selectBox);
                    option.attr('selected', 'selected');
                    selectBox.closest('.select-wrapper').find('span').html(option.text());
                }

            }
        },
        init: function($container) {
            $.ajax({
                'dataType': 'json',
                'url': '/typo3conf/ext/lw_sma_contacts/Classes/Scripts/GetCountryIso2Code.php',
                'success': function (data) {
                    if (data.country !== undefined && data.country.code !== undefined && data.country.code.length) {
                        LW.LwSmaContacts.changeCountry($container, data.country.code);
                    }
                }
            });
        }
    };

    $(function () {
        var $elements = $('.tx-lw-sma-contacts');
        if ($elements.length) {
            $elements.each(function (idx, container) {
                new LW.LwSmaContacts.init($(container));
            });
        }
    });

