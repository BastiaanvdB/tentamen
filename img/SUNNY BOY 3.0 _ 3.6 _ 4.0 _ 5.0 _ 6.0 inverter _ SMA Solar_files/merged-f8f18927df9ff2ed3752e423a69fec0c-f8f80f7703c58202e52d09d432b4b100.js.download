var LwSmaExtendedWarrentyForm = {
	$form: null,
	devicePointer: 1,

	init: function() {
		LwSmaExtendedWarrentyForm.$form = jQuery('form');
		LwSmaExtendedWarrentyForm.devicePointer = 1;

		if (jQuery('#lwSmaExtendedWarrantyForm-form-deviceItemsContainer').length > 0) {
            jQuery('.datepicker').datepicker({orientation: 'bottom', language: window.SMA.language}).removeClass('parsley-error').parent().removeClass('error');
			LwSmaExtendedWarrentyForm.bindEvents();
		}
	},

	'bindEvents' : function() {
		jQuery('#lwExtendedWarrantyForm-form-devices-addDevice').click(function (e) {
			e.preventDefault();
			jQuery('#lwExtendedWarrantyForm-form-devices-removeDevice').fadeIn('fast');

			// next device
			LwSmaExtendedWarrentyForm.devicePointer = (LwSmaExtendedWarrentyForm.devicePointer + 1);
			var deviceSelector = '#lwSmaExtendedWarrantyForm-form-deviceItemsContainer-deviceItem-' + (LwSmaExtendedWarrentyForm.devicePointer);
			if(LwSmaExtendedWarrentyForm.devicePointer < 11) {
				LwSmaExtendedWarrentyForm.toogleDeviceContainerBindEvent(deviceSelector);

				// if the number of devices is 10 fade out the add device button otherwise
				if (LwSmaExtendedWarrentyForm.devicePointer == 10) {
					jQuery('#lwExtendedWarrantyForm-form-devices-addDevice').fadeOut('fast');
				}
			}
		});

		jQuery('#lwExtendedWarrantyForm-form-devices-removeDevice').click(function (e) {
			e.preventDefault();
			jQuery('#lwExtendedWarrantyForm-form-devices-addDevice').fadeIn('fast');

			// previous device
			if (LwSmaExtendedWarrentyForm.devicePointer > 0) {
				var deviceSelector = '#lwSmaExtendedWarrantyForm-form-deviceItemsContainer-deviceItem-' + (LwSmaExtendedWarrentyForm.devicePointer);
				LwSmaExtendedWarrentyForm.toogleDeviceContainerBindEvent(deviceSelector);
				LwSmaExtendedWarrentyForm.devicePointer = LwSmaExtendedWarrentyForm.devicePointer - 1;

				// if the number of devices is 10 fade out the add device button otherwise
				if (LwSmaExtendedWarrentyForm.devicePointer == 1) {
					jQuery('#lwExtendedWarrantyForm-form-devices-removeDevice').attr('style', 'display: none !important;');
				}
			}
		});
	},

	'toogleDeviceContainerBindEvent' : function(deviceContainerSelector) {
		var $deviceContainer = jQuery(deviceContainerSelector);

		if ($deviceContainer.length > 0) {
			if(jQuery(deviceContainerSelector).is(':hidden')) {
				$deviceContainer.fadeIn('fast');
				$deviceContainer.find('input, select').removeAttr('disabled');
                $deviceContainer.find('div.select-helper').removeClass('disabled');
			} else {
				$deviceContainer.fadeOut('fast');
				$deviceContainer.find('input, select').attr('disabled', 'disabled');
                $deviceContainer.find('div.select-helper').addClass('disabled');
			}
		}
	}
};

jQuery(document).ready(function() {
	if (jQuery('#lwSmaExtendedWarrentyForm').length > 0) {
		LwSmaExtendedWarrentyForm.init();
	}
});
// prevent double submission of powermail forms by disabling it after parsley validated all fields
if (window.Parsley) {
    window.Parsley.on('form:submit', function () {
        if (this.$element.hasClass('powermail_form')) {
            var $submitButton = $(this.$element).find('.powermail_submit');
            $submitButton.attr('disabled', 'disabled').addClass('disabled-btn');

            // reenable the button according to https://docs.typo3.org/typo3cms/extensions/powermail/ForAdministrators/BestPractice/PreventDuplicates/Index.html
            setTimeout(function () {
                $submitButton.removeAttr('disabled').removeClass('disabled-btn');
            }, 6000);
        }
    });
}

