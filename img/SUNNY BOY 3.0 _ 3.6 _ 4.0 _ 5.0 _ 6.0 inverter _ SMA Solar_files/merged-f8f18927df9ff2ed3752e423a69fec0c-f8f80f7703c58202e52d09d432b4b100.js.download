/*global jQuery*/

window.LIGHTWERK = window.LIGHTWERK || {};

(function ($, LW) {
    'use strict';

    LW.LwSmaDeveloperDownloads = {

        contentElId: 0,

        showSpinner: function () {
            $('#ajax-loading').show();
            $('body').css('opacity', 0.5);
        },

        hideSpinner: function () {
            $('#ajax-loading').hide();
            $('body').css('opacity', 1);
        },

        hideFormShowFiles: function () {
            var downloadForm = $('#lw-sma-developer-downloads-form').closest('div');

            downloadForm.fadeOut('slow', function () {
                $('#c' + LW.LwSmaDeveloperDownloads.contentElId).fadeIn('slow');
                LW.LwSmaDeveloperDownloads.scrollToContent();
            });
        },

        triggerToTop: function () {
            $('#scrollTop').trigger('click');
        },

        scrollToContent: function () {
            $('html, body').animate({
                scrollTop: $('#c' + LW.LwSmaDeveloperDownloads.contentElId).offset().top - $('#navigation').height() - 20
            }, 2000);
        },

        watch: function ($container) {
            LW.LwSmaDeveloperDownloads.watchItems();
            $container.on('submit', 'form', function(e) {
                e.preventDefault();
                LW.LwSmaDeveloperDownloads.submit($container, $(this));
            });
        },

        watchItems: function () {
            var fileDownload = $('#c' + LW.LwSmaDeveloperDownloads.contentElId),
                industryContainer = $('#industry-wrapper');

            fileDownload.hide();
            industryContainer.hide();

            $('input').on('ifChecked', function(event){
                if ($(this).hasClass('useage-trigger') && ($(this).val() === '2')) {
                    industryContainer.slideDown();
                } else if ($(this).hasClass('useage-trigger') && ($(this).val() === '1')) {
                    industryContainer.slideUp();
                }
            });

        },

        submit: function ($container, $form) {
            LW.LwSmaDeveloperDownloads.showSpinner();
            $('.lw-sma-developer-downloads-form-status').hide();

            $.ajax({
                'type': 'POST',
                'url': $form.attr('action'),
                'data': $form.serialize(),
                'success': function (response) {
                    var $result = $(response).find('.lw-sma-developer-downloads-form-status');
                    if ($result.data('downloadform-status') === 1) {
                        LW.LwSmaDeveloperDownloads.hideSpinner();
                        LW.LwSmaDeveloperDownloads.hideFormShowFiles();
                    }

                    if ($result.data('downloadform-status') === 0) {
                        LW.LwSmaDeveloperDownloads.hideSpinner();
                        LW.LwSmaDeveloperDownloads.triggerToTop();
                        $container.prepend($result);
                    }

                },
                'error': function() {
                    LW.LwSmaDeveloperDownloads.hideSpinner();
                    $container.prepend('FATAL ERROR');
                }
            });
        },

        init: function($container) {
            LW.LwSmaDeveloperDownloads.contentElId = $('.content-el-id').data('content-el-id');
            LW.LwSmaDeveloperDownloads.watch($container);
        },

        onload: function() {
            var $lwSmaDeveloperDownloadsForm = $('.tx-lw-sma-developer-downloads');
            if ($lwSmaDeveloperDownloadsForm.length) {
                $lwSmaDeveloperDownloadsForm.each(function (idx, container) {
                    LW.LwSmaDeveloperDownloads.init($(container));
                });
            }
        }
    };

    $(function () {
        LW.LwSmaDeveloperDownloads.onload();
    });

}(jQuery, window.LIGHTWERK));
// Prevents the Form being sent twice by disabling it after submitting
$(document).ready(function() {
    var $form = $('[data-registration-form="prevent-doubleclick"]');
    if ($form.length > 0) {
        $form.submit(function() {
            var submitButtons = $form.find('[type="submit"]');
            submitButtons.each(function(key, button) {
                // The button is disabled, so the form can not be submitted multiple times
                // The page is reloaded anyway, so there is no need to enable the button after a timeout
                $(button).attr('disabled', 'disabled').addClass('disabled-btn');
            });
        });
    }
});
